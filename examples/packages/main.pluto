import mathlib

fn main() {
    // Use functions from our mathlib dependency
    print("=== Package Dependencies ===")

    let sum = mathlib.add(10, 20)
    print("add(10, 20) = {sum}")

    let product = mathlib.multiply(6, 7)
    print("multiply(6, 7) = {product}")

    let p = mathlib.Point { x: 3, y: 4 }
    let dist = mathlib.distance(p)
    print("distance(3, 4) = {dist}")
}
