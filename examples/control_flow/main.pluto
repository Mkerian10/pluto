fn fizzbuzz(n: int) string {
    if n % 15 == 0 {
        return "fizzbuzz"
    }
    if n % 3 == 0 {
        return "fizz"
    }
    if n % 5 == 0 {
        return "buzz"
    }
    return f"{n}"
}

fn main() {
    // Range-based FizzBuzz
    for i in 1..=20 {
        print(fizzbuzz(i))
    }

    let fruits = ["apple", "banana", "cherry"]
    for fruit in fruits {
        print("I like " + fruit)
    }

    // Sum 1..=100 using a range
    let sum = 0
    for n in 1..=100 {
        sum = sum + n
    }
    print(sum)

    // Find first multiple of 7 using break
    for i in 1..100 {
        if i % 7 == 0 {
            print(f"first multiple of 7: {i}")
            break
        }
    }

    // Skip even numbers using continue
    print("odd numbers under 10:")
    for i in 1..10 {
        if i % 2 == 0 {
            continue
        }
        print(i)
    }
}
