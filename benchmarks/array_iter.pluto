fn main() {
    let arr = [0]
    let i = 0
    while i < 1000000 {
        arr.push(i)
        i = i + 1
    }
    let start = time_ns()
    let sum = 0
    for x in arr {
        sum = sum + x
    }
    let elapsed = time_ns() - start
    let ms = elapsed / 1000000
    print("sum = {sum}")
    print("elapsed: {ms} ms")
}
