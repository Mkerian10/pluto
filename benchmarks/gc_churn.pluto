class Obj {
    value: int
    tag: int
}

fn main() {
    let n = 1000000
    let start = time_ns()
    let i = 0
    while i < n {
        let tmp = Obj { value: i, tag: 42 }
        i = i + 1
    }
    let elapsed = time_ns() - start
    let ms = elapsed / 1000000
    let heap = gc_heap_size()
    print("allocated {n} objects")
    print("heap_size: {heap} bytes")
    print("elapsed: {ms} ms")
}
