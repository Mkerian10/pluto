class Counter {
    value: int

    fn get(self) int {
        return self.value
    }
}

fn main() {
    let start = time_ns()
    let mut c = Counter { value: 0 }
    let mut i = 0
    while i < 10000000 {
        c.value = c.value + 1
        i = i + 1
    }
    let elapsed = time_ns() - start
    let ms = elapsed / 1000000
    let val = c.get()
    print("value = {val}")
    print("elapsed: {ms} ms")
}
